{
  "name": "tech-transfer-backend",
  "version": "1.0.0",
  "description": "Backend service for technology transfer data aggregation and grant-writing assistance system with integrated web scraping, AI processing, and data management capabilities",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts",
    "test:integration": "jest --config jest.integration.config.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "docker:build": "docker build -t tech-transfer-backend .",
    "docker:run": "docker run -p 3000:3000 tech-transfer-backend",
    "clean": "rimraf dist coverage",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
